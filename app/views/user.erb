<%= erb :_navbar %>



<div id="button_container">
  <button class="add">Add to your collection</button>
</div>

<div id="poup_search">
  <%= erb :_search %>
</div>

<div class="user_box" id="user_links">
  <h3>Your Poups</h3>
  <% @links.each do |link| %>
  <p><a href="<%= link.url %>"><%= link.url %></a></p>
  <% end %>
</div>

<iframe class="iframe" width="560" height="315" src="https://www.youtube.com/embed/HFzTm6fy_zs" frameborder="0" allowfullscreen>
</iframe>



<div class="user_box" id="recent_links">
  <h3>Global Poups</h3>
  <% @all_links.each do |link| %>
  <p><a href="<%= link.url %>"><%= link.url %></a></p>
  <% end %>
</div>


